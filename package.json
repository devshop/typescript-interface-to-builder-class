{
  "name": "typescript-interface-to-builder-class",
  "displayName": "TypeScript Interface to Builder Class",
  "description": "Automatically generate a builder class from a TypeScript interface",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.35.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.interfaceToBuilder"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.interfaceToBuilder",
        "title": "Generate Builder Class From TypeScript Interface"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "prettier": "1.18.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  }
}
